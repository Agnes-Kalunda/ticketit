{
    "name": "ticket/ticketit",
    "description": "Your custom ticket management system for Laravel",
    "version":"1.0.4",
    "type": "laravel-package",
    "keywords": ["laravel", "helpdesk", "ticket", "support", "your-keywords"],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "5.8.*",
        "laravelcollective/html": "5.8.*",
        "illuminate/support": "5.8.*",
        "yajra/laravel-datatables-oracle": "^9.4",
        "jenssegers/date": "^3.0",
        "mews/purifier": "^2.0",
        "doctrine/dbal": "^2.10"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Agnes",
            "email": "agypeter97@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ticket\\Ticketit\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ticket\\Ticketit\\TicketitServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.5"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}